{
  "name": "@manifesto-ai/react-migrate",
  "version": "0.1.0",
  "description": "Agent-powered React to Manifesto migration tool",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "react-migrate": "./bin/react-migrate.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    },
    "./parser": {
      "types": "./dist/parser/index.d.ts",
      "import": "./dist/parser/index.js"
    },
    "./llm": {
      "types": "./dist/llm/index.d.ts",
      "import": "./dist/llm/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "cli": "tsx src/cli/cli.tsx"
  },
  "peerDependencies": {
    "@manifesto-ai/core": "^0.3.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@swc/core": "^1.10.1",
    "better-sqlite3": "^11.7.0",
    "globby": "^14.0.2",
    "ink": "^5.1.0",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "meow": "^13.2.0",
    "ollama": "^0.5.11",
    "openai": "^4.77.0",
    "react": "^18.3.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@manifesto-ai/core": "^0.3.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@vitest/coverage-v8": "^2.1.9",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.9"
  },
  "keywords": [
    "manifesto",
    "ai",
    "react",
    "migration",
    "agent",
    "cli",
    "ast",
    "swc"
  ],
  "author": "Manifesto Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manifesto-ai/tools.git",
    "directory": "packages/react-migrate"
  },
  "homepage": "https://github.com/manifesto-ai/tools/tree/main/packages/react-migrate#readme",
  "bugs": {
    "url": "https://github.com/manifesto-ai/tools/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
